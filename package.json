        "axios": "^0.20.0",
        "chai": "^4.3.4",
        "cors": "^2.8.5",
        "ethereum-waffle": "^3.4.0",
        "ethers": "^5.4.7",
        "express": "^4.17.1",
        "ganache": "^6.4.5",
        "hardhat": "^2.6.5",
        "mocha": "^9.1.2",
